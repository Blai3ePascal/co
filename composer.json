{
    "name": "mpyw/co",
    "description": "Asynchronous cURL executor simply based on resource and Generator",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "curl",
        "http",
        "asynchronous",
        "parallel",
        "generator"
    ],
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "autoload": {
        "psr-4": {
            "mpyw\\Co\\": "src/"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "lib-curl": ">=7.2.0",
        "mpyw/runtime-promise": "1.0"
    },
    "require-dev": {
        "php": ">=7.0.0",
        "lib-curl": ">=7.2.0",
        "mpyw/twistoauth": "~3.0",
        "codeception/aspect-mock": "^2.0",
        "codeception/codeception": "^2.2",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "satooshi/php-coveralls": "^1.0",
        "mpyw/privator": "^2.0"
    },
    "scripts": {
        "test": [
            "php vendor/bin/codecept run unit --coverage --coverage-xml"
        ]
    },
    "archive": {
        "exclude": [
            "*",
            ".*",
            "!/src/*"
        ]
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "mpyw/runtime-promise",
                "version": "1.0",
                "source": {
                    "url": "https://github.com/mpyw/runtime-promise.git",
                    "type": "git",
                    "reference": "4241a89d83f1e4fc4bfb7ab7487a0ef0abc5a35f"
                },
                "autoload": {
                    "psr-4": {
                        "mpyw\\RuntimePromise\\": "src/"
                    },
                    "files": ["src/functions_include.php"]
                }
            }
        }
    ]
}
